{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/server.js"
    },
    {
      "source": "/js/(.*)",
      "destination": "/frontend/js/$1"
    },
    {
      "source": "/assets/(.*)",
      "destination": "/assets/$1"
    },
    {
      "source": "/",
      "destination": "/frontend/pages/Home.html"
    },
    {
      "source": "/(.*.html)",
      "destination": "/frontend/pages/$1"
    }
  ]
}